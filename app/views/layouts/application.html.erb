<!DOCTYPE html>
<html>
  <head>
    <title>KingpingApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <h1 class="headlines">Bitmaker KingPing</h1>
      <nav class="navbar">
        <ul class="navlist">
          <li class="navlistelement">
            <%= link_to "Players", players_path %>
          </li>
          <li>
            <%= link_to "Games People Play", games_url %>
          </li>
          
        </ul>
      </nav>

    </header>
    <%= yield %>
  </body>
</html>
